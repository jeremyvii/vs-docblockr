{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "indent": [true, "spaces", 2],
        "semicolon": [true, "alway"],
        "quotemark": [true, "single"],
        "no-unused-variable": false,
        "curly": false,
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-leading-underscore"
        ],
        "interface-name": [false],
        "radix": false,
        "typedef-whitespace": [false]
    },
    "rulesDirectory": []
}